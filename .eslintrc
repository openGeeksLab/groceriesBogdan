{
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "plugins": [
        "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "rallycoding"
    ],
    "rules": {
      "react/require-extension": 0,
      "no-else-return": ["error", { "allowElseIf": false }],
    }
}
